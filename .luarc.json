{
  "runtime.version": "Lua 5.1",
  "runtime.path": [
    "?.lua",
    "?/init.lua"
  ],
  "workspace.library": [
    "${3rd}/lua-language-server/library",
    "${workspace}/types"
  ],
  "workspace.checkThirdParty": false,
  "diagnostics.globals": [
    "game",
    "workspace",
    "script",
    "shared",
    "ReplicatedStorage",
    "ReplicatedFirst",
    "StarterGui",
    "StarterPack",
    "StarterPlayer",
    "StarterCharacterScripts",
    "StarterPlayerScripts",
    "ServerScriptService",
    "ServerStorage",
    "SoundService",
    "Lighting",
    "Players",
    "Teams",
    "Debris",
    "InsertService",
    "Chat",
    "HttpService",
    "TeleportService",
    "TweenService",
    "UserInputService",
    "RunService",
    "ContentProvider",
    "AssetService",
    "DataStoreService",
    "MessagingService",
    "PathfindingService",
    "PhysicsService",
    "CollectionService",
    "ContextActionService",
    "GuiService",
    "MarketplaceService",
    "ProximityPromptService",
    "TextChatService",
    "VoiceChatService",
    "VRService",
    "VirtualUser",
    "print",
    "warn",
    "error",
    "wait",
    "delay",
    "spawn",
    "tick",
    "time",
    "type",
    "typeof",
    "Instance",
    "Vector3",
    "Vector2",
    "CFrame",
    "Color3",
    "BrickColor",
    "UDim2",
    "UDim",
    "Ray",
    "TweenInfo",
    "NumberSequence",
    "NumberSequenceKeypoint",
    "ColorSequence",
    "ColorSequenceKeypoint",
    "Region3",
    "Faces",
    "Axes",
    "Enum",
    "task",
    "table",
    "string",
    "math",
    "os"
  ],
  "diagnostics.disable": [],
  "diagnostics.severity": {
    "unused-local": "Information",
    "unused-function": "Information",
    "undefined-field": "Warning",
    "undefined-global": "Warning",
    "redefined-local": "Warning",
    "unreachable-code": "Warning",
    "trailing-space": "Information",
    "redundant-parameter": "Information",
    "redundant-value": "Information",
    "ambiguity-1": "Warning",
    "lowercase-global": "Information",
    "undefined-doc-name": "Warning",
    "undefined-doc-class": "Warning",
    "undefined-doc-param": "Warning",
    "duplicate-doc-alias": "Warning",
    "duplicate-doc-param": "Warning",
    "duplicate-set-field": "Warning"
  },
  "completion.enable": true,
  "completion.callSnippet": "Both",
  "completion.keywordSnippet": "Both",
  "completion.displayContext": 0,
  "completion.workspaceWord": true,
  "completion.autoRequire": true,
  "completion.showWord": "Enable",
  "completion.postfix": "@",
  "color.mode": "Grammar",
  "hover.enable": true,
  "hover.viewString": true,
  "hover.viewStringMax": 1000,
  "hover.viewNumber": true,
  "hover.fieldInfer": true,
  "hover.previewFields": 5,
  "signatureHelp.enable": true,
  "codeLens.enable": false,
  "intelliSense.searchDepth": 5,
  "intelliSense.fastGlobal": false,
  "format.enable": false,
  "format.defaultConfig": {
    "indent_style": "space",
    "indent_size": "4",
    "continuation_indent_size": "4"
  },
  "spell.dict": null,
  "files.associations": {
    "*.lua": "lua"
  },
  "files.exclude": [],
  "files.ignoreSubModules": false,
  "files.maxSize": 1024
}

